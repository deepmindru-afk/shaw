{
  "name": "Roadtrip - Main CI",
  "description": "Build and test every push to main.",
  "containerFilePath": "Roadtrip.xcodeproj",
  "isEnabled": true,
  "isLockedForEditing": false,
  "clean": false,
  "branchStartCondition": {
    "source": {
      "isAllMatch": false,
      "patterns": [
        {
          "pattern": "main",
          "isPrefix": false
        }
      ]
    },
    "filesAndFoldersRule": {
      "mode": "START_IF_ANY_FILE_MATCHES",
      "matchers": []
    },
    "autoCancel": true
  },
  "actions": [
    {
      "name": "Archive Roadtrip",
      "actionType": "ARCHIVE",
      "scheme": "Roadtrip",
      "platform": "IOS",
      "isRequiredToPass": true
    }
  ]
}
